{
  "name": "electron-redux-first-boilerplate",
  "version": "0.0.1",
  "description": "Haz los reportes de SEMADET usando s√≥lo los xml de las facturas del semestre",
  "main": "main.js",
  "engines": {
    "node": "8.11.4"
  },
  "scripts": {
    "test": "node main.js",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --config webpack.prod.js",
    "web": "npm run clean && cross-env WEB=true webpack-dev-server --open --config webpack.dev.js",
    "watch": "webpack --watch --config webpack.dev.js",
    "electron": "electron .",
    "electron:prod": "cross-env NODE_ENV=production electron .",
    "start": "npm run clean && cross-env NODE_ENV=development node --trace-warnings ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.dev.js",
    "clean": "rimraf dist/",
    "prod": "npm run build && npm run electron:prod"
  },
  "author": "Martiuh",
  "license": "ISC",
