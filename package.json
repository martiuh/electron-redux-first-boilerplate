    "glamor": "^2.20.40",
    "glamorous": "^4.13.1",
    "history": "^4.7.2",
    "lodash": "^4.17.4",
    "pixl-xml": "^1.0.10",
    "qs": "^6.5.2",
    "react": "^16.3.2",
    "react-hot-loader": "^4.3.3",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
